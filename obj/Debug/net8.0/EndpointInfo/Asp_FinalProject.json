  "components": {
    "schemas": {
      "Movie": {
        "required": [
          "category",
          "description",
          "posterUrl",
          "releaseDate",
          "title"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "title": {
            "maxLength": 100,
            "minLength": 2,
            "type": "string"
          },
          "description": {
            "maxLength": 500,
            "minLength": 0,
            "type": "string"
          },
          "posterUrl": {
            "maxLength": 200,
            "minLength": 0,
            "pattern": ".*\\.(jpg|jpeg|png|gif|webp)$",
            "type": "string",
            "format": "uri"
          },
          "releaseDate": {
            "type": "string",
            "format": "date"
          },
          "budget": {
            "minimum": 1,
            "type": "number",
            "format": "double"
          },
          "category": {
            "maxLength": 100,
            "minLength": 0,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "RegisterDTO": {
        "required": [
          "email",
          "fullName",
          "password",
          "role"
        ],
        "type": "object",
        "properties": {
          "email": {
            "minLength": 1,
            "type": "string",
            "format": "email"
          },
          "password": {
            "minLength": 1,
            "type": "string"
          },
          "fullName": {
            "minLength": 1,
            "type": "string"
          },
          "role": {
            "minLength": 1,
            "pattern": "^(Admin|Reader)$",
            "type": "string"
          }
        },
        "additionalProperties": false
      }
    },
    "securitySchemes": {
      "Bearer": {
        "type": "apiKey",
        "description": "Enter 'Bearer' [space] and then your valid token.\n\nExample: \"Bearer eyJhbGciOiJI...\"",
        "name": "Authorization",
        "in": "header"
      }
    }
  },
  "security": [
    {
      "Bearer": [ ]
    }
  ]
}